[Unit]
Description=Consul server process
Requires=network-online.target
After=network-online.target

[Service]
Type=simple
Restart=on-failure
ExecStart=/opt/consul/consul agent -config-dir /etc/consul.d -data-dir /tmp/consul-test 
KillSignal=SIGTERM
User=root
Group=root

[Install]
WantedBy=multi-user.target

